<main class="main">
  <div class="inner">
    <h1>トップページ</h1>
    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは、
        <%= link_to "#{@user.name}", user_path(@user), class: :greeting__link %>
      </div>

      <div class="card__wrapper">
        <%= render partial: 'prototype', collection: @prototypes %>
      </div>
    <% else%>
      <div class="card__wrapper">
        <%= render partial: 'prototype', collection: @prototypes %>
      </div>  
    <% end %>  
  </div>
</main>
